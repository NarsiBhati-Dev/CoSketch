generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  username String @unique
  password String
  name String?
  createdAt DateTime @default(now())
}

model Room {
  id String @id 
  RoomId String
  slug String
  createdAt DateTime @default(now())
}